<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<servers>
		<!-- 
 			====================================
 				Repositories
 		    ==================================== 
 		-->
 		<!-- nexus repo -->
 		<server>
 			<id>tupadr3-releases</id>
 			<username>tupadr3</username>
 			<password>XXXXXXXXXXXXXXXXXXXXXX</password>
 		</server>

 		<server>
 			<id>tupadr3-snapshots</id>
 			<username>tupadr3</username>
 			<password>XXXXXXXXXXXXXXXXXXXXXX</password>
 		</server>

 		<server>
 			<id>tupadr3-nexus</id>
 			<username>tupadr3</username>
 			<password>XXXXXXXXXXXXXXXXXXXXXX</password>
 		</server>

 		<!-- docker settings -->
 		<server>
 			<id>tupadr3-docker-registry</id>
 			<username>tupadr3</username>
 			<password>XXXXXXXXXXXXXXXXXXXXXX</password>
 			<configuration>
 				<email>dev@tupadr3.de</email>
 			</configuration>
 		</server>
 		<server>
 			<id>tupadr3-docker-mirror</id>
 			<username>tupadr3</username>
 			<configuration>
 				<email>ci@tupadr3.de</email>
 			</configuration>
 		</server>
 	</servers>
 	<mirrors>
 		<mirror>
 			<id>tupadr3-nexus</id>
 			<mirrorOf>*</mirrorOf>
 			<url>https://nexus.tupadr3.de/repository/maven-public/</url>
 		</mirror>
 	</mirrors>
 	<profiles>
 		<profile>
 			<id>tupadr3-nexus</id>
 			<repositories>
 				<repository>
 					<id>tupadr3-releases</id>
 					<name>Build Repo</name>
 					<url>https://nexus.tupadr3.de/repository/maven-releases</url>
 					<releases>
 						<updatePolicy>always</updatePolicy>
 						<enabled>true</enabled>
 					</releases>
 					<snapshots>
 						<enabled>false</enabled>
 					</snapshots>
 				</repository>
 				<repository>
 					<id>tupadr3-snapshots</id>
 					<name>Snapshot Repo</name>
 					<url>https://nexus.tupadr3.de/content/repositories/maven-snapshots</url>
 					<snapshots>
 						<updatePolicy>always</updatePolicy>
 						<enabled>true</enabled>
 					</snapshots>
 					<releases>
 						<enabled>false</enabled>
 					</releases>
 				</repository>
 				<repository>
 					<id>central</id>
 					<url>http://central</url>
 					<releases><enabled>true</enabled></releases>
 					<snapshots><enabled>true</enabled></snapshots>
 				</repository>
 			</repositories>
 			<pluginRepositories>
 				<pluginRepository>
 					<id>central</id>
 					<url>http://central</url>
 					<releases><enabled>true</enabled></releases>
 					<snapshots><enabled>true</enabled></snapshots>
 				</pluginRepository>
 			</pluginRepositories>
 		</profile>
 	</profiles>
 	<activeProfiles>
 		<activeProfile>tupadr3-nexus</activeProfile>
 	</activeProfiles>
 </settings>